def find_factors(n):
    factors = []
    for i in range(1, int(n**0.5) + 1):
        if n % i == 0:
            factors.append(i)
            if i != n // i:
                factors.append(n // i)
    return sorted(factors)
def find_pth_factor(n, p):
    factors = find_factors(n)
    if p <= len(factors):
        return factors[p - 1]
    else:
        return 0
n = int(input())
p = int(input())
result = find_pth_factor(n, p)
print(result)
